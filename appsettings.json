{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "WebServer": {
      "AddServerHeader": false
    }
  },
  "HostConfiguration": {
    "Server": {
      "RemoveServerHeader": true
    }
  },
  "Security": {
    "RemoveHeaders": {
      "X-Powered-By": true,
      "Server": true,
      "X-AspNet-Version": true
    }
  },

  "AllowedHosts": "*"
}
